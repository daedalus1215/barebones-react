{
  "name": "barebones",
  "version": "0.0.1",
  "description": "the ui for making Disney Reservation events",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development parcel index.html --open",
    "build": "NODE_ENV=production parcel build index.html --public-url ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:eslint": "eslint src/ --ext .js --ext .jsx",
    "format": "prettier --write ."
  },
  "keywords": [
    "react"
  ],
  "author": "LA",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^7.4.0",
    "node-sass": "^7.0.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "dependencies": {
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  }
}
